class lifo;
  int intQ[$];
  function void put(int a);
    intQ.push_back(a);
  endfunction
  function void getlifo(output int a);
   a=intQ.pop_front();
  endfunction
   function void getfifo(output int a);
   a=intQ.pop_back();
  endfunction
 endclass
   module tb;
     lifo l=new();
     int num;
     initial begin
       repeat(5) begin
         num=$urandom_range(100,200);
         l.put(num);
         $display("num=%0d",num);
       end
       repeat(5) begin
         l.getlifo(num);
         $display("num=%0d",num);
       end
       repeat(5) begin
         l.getfifo(num);
         $display("num=%0d",num);
       end
     end
       endmodule
